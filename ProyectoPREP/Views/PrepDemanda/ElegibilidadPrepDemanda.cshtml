		$(document).ready(function () {
			$('#txtNombre, #txtApellido, #txtNombreContacto').on('input', function () {
				var inputValue = $(this).val();
				var onlyLetters = /^[A-Za-zÁÉÍÓÚáéíóúÑñ\s]+$/.test(inputValue);

				if (!onlyLetters) {
					// Si se introduce algún número, limpiamos el valor del campo
					$(this).val(inputValue.replace(/[^A-Za-zÁÉÍÓÚáéíóúÑñ\s]/g, ''));
				}
			});
		});


		$(document).ready(function () {
			$('#txtPeso, #txtTallaPies, #TallaPulgadas').on('input', function () {
				var inputValue = $(this).val();
				// Reemplaza todo lo que no es un número con una cadena vacía
				$(this).val(inputValue.replace(/[^\d]/g, ''));
			});
		});



		//VALIDACION PARA RESTRINGIR QUE NO SE INGRESEN MENORES DE EDAD

		function applyDateRestriction() {
			const dateInput = document.getElementById('txtFechaNacimiento');
			const isRestricted = localStorage.getItem('restrictDate') !== 'true';
			const today = new Date();
			const maxDate = new Date(today.getFullYear() - 18, today.getMonth(), today.getDate());
			const formattedMaxDate = maxDate.toISOString().split('T')[0];

			if (isRestricted) {
				dateInput.max = formattedMaxDate;
			} else {
				dateInput.max = '';
			}
		}

		// Aplicar restricción al cargar la página
		document.addEventListener('DOMContentLoaded', (event) => {
			applyDateRestriction();
		});

		// Escuchar cambios en localStorage para actualizar la restricción de fecha dinámicamente
		window.addEventListener('storage', (event) => {
			if (event.key === 'restrictDate') {
				applyDateRestriction();
			}
		});

